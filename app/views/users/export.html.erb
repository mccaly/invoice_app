<% provide(:title, "Export data") %> 

<div class="row" style="margin-top: 20px;">
  <div class="container">
  <div class="span6 offset3">
    <%= form_tag(controller: "users", action:"export_csv", method: :get) %>
      <div class ="span2">
        <p>Date to begin export</p>
          <%= date_select  :users, :start_date %>
      </div>

      <div class="span2">
        <p>Date to end export</p>
          <%= date_select :users, :end_date %>
      </div>
      <div class="span6">
      <%= submit_tag("export", class: "btn btn-large")  %>
      </div>  
  </div>
</div>
</div>