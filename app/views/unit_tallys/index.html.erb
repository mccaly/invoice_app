<% provide(:title, "tally") %> 
<h1>tally</h1>

<div class="row">
 <div class="span12 offset2">
	<table class="table table-hover">
 		<thead>
 			<tr>
 				<th>date created</th>
 				<th>access token</th>
 				<th>quantity</th>
 				<th>amount/ unit</th>
 				<th>amount total</th>
 			</tr>
 		</thead>

 		
 		<tbody>
 		<%= @unit_tally.each do |unit_tally| %>
 			<tr>
 				<td><%= tally.access_token %></td>
 				<td><%= tally.unit_id %></td>
 				<td><%= tally.amount %></td>
 				<td><%= tally.created_at %></td>
 			</tr>
 		</tbody>
 		<% end %>
	</table>
</div>
</div>